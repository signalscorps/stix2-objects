{
    "$id": "https://raw.githubusercontent.com/signalscorps/stix2-objects/main/schemas/properties/misp-warning-list-match/schema.json",
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "Schema for adding Warning List Matches to Objects",
    "description": "The set of properties that define the match. For extension-definition--c8ea5ecb-f4a3-45e7-94de-9b9ba05161af",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://github.com/oasis-open/cti-stix2-json-schemas/blob/master/schemas/observables/software.json"
        },
        {
            "properties": {
                "extensions": {
                    "extension-definition--c8ea5ecb-f4a3-45e7-94de-9b9ba05161af": {
                        "$ref": "#/$defs/misp-warning-list-match-extension"
                    },
                    "required": [
                        "extension-definition--c8ea5ecb-f4a3-45e7-94de-9b9ba05161af"
                    ]
                }
            }
        },
    ],
    "$defs": {
        "properties": {
            "extension_type": {
                "type": "string",
                "description": "The value of this property MUST be `property-extension`.",
                "enum": [
                    "property-extension"
                ]
            },
            "misp_warning_list_match": {
                "type": "array",
                "description": "Matches to default MISP Warning Lists",
                "items": {
                    "type": "string",
                    "example": ["Top 20 000 websites from Cisco Umbrella","Top 10K websites from Majestic Million"]
                }
            },
            "custom_warning_list_match": {
                "type": "array",
                "description": "Matches to custom Warning Lists",
                "items": {
                    "type": "string",
                    "example": ["My custom list"]
                }
            }
        }
    }
}